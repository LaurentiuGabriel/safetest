{
  "name": "-vite-vue-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm i ../../ && (rm -rf ../../build/vite-vue-ts || true) && tsc && vite build && mv dist ../../build/vite-vue-ts",
    "preview": "vite preview",
    "safetest": "vitest --inspect --no-threads --globals -- --url=http://localhost:3000/vite-vue-ts/",
    "safetest:ci:test": "OPT_URL=https://safetest-two.vercel.app/vite-vue-ts OPT_CI=1 OPT_DOCKER=1 OPT_ARTIFACTS=artifacts.json vitest --inspect --no-threads --globals --run --bail=5",
    "safetest:ci": "(npm run safetest:ci:test || true)",
    "process:ci": "npx safetest merge-json results.json artifacts.json && cp results.json ../../build/vite-vue-ts/results.json"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "safetest": "file:../.."
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.8.5"
  }
}
