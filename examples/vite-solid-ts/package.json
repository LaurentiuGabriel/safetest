    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "safetest": "file:../..",
