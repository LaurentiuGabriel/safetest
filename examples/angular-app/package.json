{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "(rm -rf ../../build/angular-app || true) && ng build --base-href 'https://safetest-two.vercel.app/angular-app/' && mv dist/angular-app ../../build/angular-app",
    "watch": "ng build --watch --configuration development",
    "safetest": "node --inspect node_modules/.bin/jest --runInBand --url=http://localhost:4200/angular-app --testMatch '**/*.safetest.{j,t}s{,x}' --setupFilesAfterEnv ./setup-safetest.js",
    "safetest:ci": "npm run safetest -- --watchAll=false --ci=1 --docker=1 --url=https://safetest-two.vercel.app/angular-app --json --outputFile=../../build/angular-app/results.json",
    "process:ci": "node -e 'require(\"safetest/process-action\")' -- --results=../../build/angular-app/results.json --artifacts=../../build/angular-app/artifacts --url=https://safetest-two.vercel.app/angular-app --build-url=.",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "safetest": "file:../..",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/platform-browser": "^16.2.2",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.4",
    "jasmine-core": "~4.6.0",
    "jest": "^29.6.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.3"
  }
}
